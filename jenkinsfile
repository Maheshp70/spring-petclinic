pipeline{
    agent { label 'MAVEN' }
    options{
        timeout(time: 30, unit: 'Minute')
    }
    triggers{
        pollSCM('* * * * *')
    }
    stages{
        stage(git){
            steps{
                git url: 'https://github.com/Maheshp70/spring-petclinic.git',
                    branch: 'dev'
            }
        }
        stage(build){
            steps{
                sh 'clean package'
                archiveArtifacts artifacts: '**/spring-petclinic-*.jar'
                junit testResults: '**/TEST-*.xml'
        }

    }
}
}